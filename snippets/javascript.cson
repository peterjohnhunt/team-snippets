	'Log':
		'prefix': '_'
		'body': 'console.log( $1 );$2'


	'Setup':
		'prefix': 'setup'
		'body': """
			jQuery(document).ready(function($){

				$1

			});
		"""

	'Plugin':
		'prefix': 'plugin'
		'body': """
			(function($){

				$1

			})(jQuery);
		"""


	#▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤▤#
